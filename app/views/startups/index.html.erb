<%= form_tag("/startups/search", :method => "get") do %>
    <%= label_tag(:query, "Search for:") %>
    <%= text_field_tag(:query) %>
    <%= submit_tag("Search") %>
    <% end %>

<% unless @companies.nil? %>
    <% @companies.each do |company| %>
        <%= company.name %>
    <% end %>
<% end %>